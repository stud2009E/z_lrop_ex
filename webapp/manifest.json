{
	"_version": "1.7.0",
	"sap.app": {
		"id": "z.lrop.ex",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"tags": {
			"keywords": []
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/Z_LROP_EX_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": [
						"annotation0"
					]
				}
			},
			"annotation0": {
				"type": "ODataAnnotation",
				"uri": "localService/annotation.xml",
				"settings": {
					"localUri": "localService/annotation.xml"
				}
			}
		},
		"offline": false,
		"sourceTemplate": {
			"id": "LROP.worklist",
			"version": "1.41.1"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_belize"
		]
	},
	"sap.ui5": {
		"resources": {
			"js": [],
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.collaboration": {
					"lazy": false
				},
				"sap.ushell": {
					"lazy": false
				},
				"sap.uxap": {
					"lazy": false
				},
				"sap.ui.core": {
					"lazy": false
				},
				"sap.m": {
					"lazy": false
				},
				"sap.f": {
					"lazy": false
				},
				"sap.ui.generic.app": {
					"lazy": false
				},
				"sap.suite.ui.generic.template": {
					"lazy": false
				},
				"sap.ui.comp": {
					"lazy": false
				},
				"sap.ui.layout": {
					"lazy": false
				},
				"sap.ui.table": {
					"lazy": false
				},
				"sap.ui.fl": {
					"lazy": false
				}	
			},
			"components": {}
		},
		"models": {
			"@i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties"
			},
			"": {
				"dataSource": "mainService",
				"settings": {
					"useBatch": true,
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "Inline",
					"refreshAfterChange": false
				}
			}
		},
		"extends": {
			"extensions": {
				"sap.ui.viewExtensions": {
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"ReplaceFacet|HeaderSet|replaceFacetFG": {
							"type": "XML",
							"className": "sap.ui.core.mvc.View",
							"viewName": "z.lrop.ex.ext.view.Example"
						}
					}
				},
				"sap.ui.controllerExtensions": {
					"sap.suite.ui.generic.template.ListReport.view.ListReport": {
						"controllerName": "z.lrop.ex.ext.controller.ListReportExt"
					},
					"sap.suite.ui.generic.template.ObjectPage.view.Details": {
						"controllerName": "z.lrop.ex.ext.controller.ObjectPageExt",
						"sap.ui.generic.app": {
							"HeaderSet": {
								"EntitySet": "HeaderSet",
								"Header": {
									"Actions": {}
								}
							}
						}
					}
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		}
	},
	"sap.ui.generic.app": {
		"_version": "1.3.0",
		"settings": {
			"showDraftToggle": false,
			"objectPageHeaderType": "Dynamic",
			"forceGlobalRefresh": false,
			"isFiori3Enabled": true
		},
		"pages": {
			"ListReport|HeaderSet": {
				"entitySet": "HeaderSet",
				"component": {
					"name": "sap.suite.ui.generic.template.ListReport",
					"list": true,
					"settings": {
						"condensedTableLayout":true,
						
						"smartVariantManagement": true,
						"variantManagementHidden": false,
						"enableTableFilterInPageVariant": true,

						"tableSettings": {
							"type": "GridTable",
							"multiSelect": true
						},

						"quickVariantSelectionX": {
							"showCounts": true,
							"variants": {
								"header": {
									"key": "header",
									"entitySet": "HeaderSet",
									"annotationPath": "com.sap.vocabularies.UI.v1.SelectionPresentationVariant#header"
								},
								"file": {
									"key": "file",
									"entitySet": "FileSet",
									"title": "{{appDescription}}",
									"annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant"
								}
							}
						}
					}
				},
				"pages": {
					"ObjectPage|HeaderSet": {
						"entitySet": "HeaderSet",
						"component": {
							"name": "sap.suite.ui.generic.template.ObjectPage",
							"settings": {}
						}
					}
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": ""
	}
}