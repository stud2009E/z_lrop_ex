			<Annotations Target="Metadata.HeaderType">
				<Annotation Term="UI.LineItem" Qualifier="header">
					<Collection>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="Guid"/>
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value" Path="Date"/>
