									<Collection>
										<Record Type="Common.SortOrderType">
											<PropertyValue Property="Property" PropertyPath="Int16"/>
											<PropertyValue Property="Descending" Bool="false"/>
										</Record>
									</Collection>
								</PropertyValue>
							</Record>
						</PropertyValue>
					</Record>
